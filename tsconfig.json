{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es2018",
      "lib": ["es2018"],
      "noImplicitAny": false,
      "moduleResolution": "node",
      "sourceMap": false,
      "outDir": "dist",
      "rootDir": "./src",
      "baseUrl": "./src",
      "paths": {
          "@*": ["./*"],
          "@app/*": ["./app/*"],
          "@middlewares/*": ["./middlewares/*"],
          "@models/*": ["./models/*"],
          "@services/*": ["./services/*"],
          "@utils/*": ["./utils/*"]
      }
  },
  "include": [
    "src"
  ]
}